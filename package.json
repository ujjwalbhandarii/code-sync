{
  "name": "code-sync",
  "version": "1.0.0",
  "description": "Simple Code Collaboration tool made with Angular and node.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"bun run dev:api\" \"bun run dev:web\"",
    "dev:web": "cd apps/web && bun dev",
    "dev:api": "cd apps/server && bun run dev",
    "install:all": "bun install && bun install:web && bun install:api",
    "install:web": "cd apps/web && bun install",
    "install:api": "cd apps/server && bun install"
  },
  "keywords": [],
  "author": "ujjwalbhandarii",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
